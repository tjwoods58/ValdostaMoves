<h1 class="gym">Gyms Near You</h1>

<script src="//maps.google.com/maps/api/js?v=3.13&sensor=false&libraries=geometry" type="text/javascript"></script>
<script src="//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js" type="text/javascript"></script>

<div class="gym" style='width: 950px;'>
 <iframe width="950" height="500" frameborder="0" style="border:0" src="https://www.google.com/maps/embed/v1/search?q=gyms%20in%20Valdosta%2C%20GA%2C%20United%20States&key=AIzaSyAuE-A4jLPY3Yio7wokIwYEMloDPSZKLJ0"></iframe> 
</div>
<script type="text/javascript">
  handler = Gmaps.build('Google');
  handler.buildMap({ provider: {}, internal: {id: 'map'}}, function(){
    markers = handler.addMarkers(<%=raw @hash.to_json %>);
    handler.bounds.extendWith(markers);
    handler.fitMapToBounds();
  });
</script>


